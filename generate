> # define the options to use
> # eg: fuse|f=i
> def self.use(*args)
>   args.flatten.each do |arg|
>     (formstr,type)=arg.split('=')
>     type||='bool'
>     forms=formstr.split('|')
>     alt=forms[0]
>     forms.each do |f|
>       h=Hash.new
>       h['alt']=alt
>       h['type']=type
>       @@opts[f.to_sym]=h
> 
>       cattr_accessor f          # defines the new accessor
32a24
> end
