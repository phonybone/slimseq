<h1>New post_pipeline</h1>

<% form_for(@post_pipeline) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :runtype %><br />
    <%= f.text_field :runtype %>
  </p>
  <p>
    <%= f.label :max_mismatches %><br />
    <%= f.text_field :max_mismatches %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', post_pipelines_path %>
